{
  "name": "nicox",
  "manifest_version": 2,
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon_32.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_scripts.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
